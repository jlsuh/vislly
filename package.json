{
  "name": "vislly",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "biome:check": "biome check .",
    "biome:ci": "biome ci .",
    "biome:format": "biome check --write --unsafe .",
    "biome:migrate": "biome migrate --write",
    "build": "tsc && next build --turbopack",
    "depc:ci": "bun depc:report && bun depc:metrics && bun depc:gen",
    "depc:gen": "depcruise . -I \"^src|^app\" -T dot | dot -T svg | depcruise-wrap-stream-in-html > ./dependency-graph.html && bun depc:open",
    "depc:genl": "depcruise . -I \"^src|^app\" -S 2 -T dot | dot -T svg | depcruise-wrap-stream-in-html > ./dependency-graph.html && bun depc:open",
    "depc:metrics": "depcruise . -I \"^src|^app\" --metrics -T metrics",
    "depc:open": "open ./dependency-graph.html || start ./dependency-graph.html",
    "depc:report": "depcruise . -I \"^src|^app\"",
    "dev": "next dev --turbopack",
    "serve": "bun run build && serve ./out",
    "start": "next start"
  },
  "dependencies": {
    "next": "15.4.1",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@types/node": "24.0.14",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "dependency-cruiser": "16.10.4",
    "serve": "14.2.4",
    "type-fest": "4.41.0",
    "typescript": "5.8.3"
  }
}
